profile studyplan-apparmor-profile flags=(attach_disconnected) {

  file,
  network,
  capability,

  /usr/bin/python3 rix,
  /usr/local/bin/uvicorn rix,
  /usr/local/bin/** rix,
  /usr/lib/** mr,

  /app/** rwklix,
  /tmp/** rwklix,

  /data/db/** rwklix,

  /proc/** r,
  deny /proc/sys/** rwklx,
  deny /proc/sysrq-trigger rwklx,

  deny /root/** rwklx,
  deny /etc/passwd rw,
  deny /etc/shadow rw,

  /sys/devices/** r,
  deny /sys/** rwklx,

  deny /** w,
}
